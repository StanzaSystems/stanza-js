{
  "extends": ["plugin:@nx/react", "../../.eslintrc.json"],
  "parserOptions": {
    "project": ["samples/react-native-simple/tsconfig*.json"]
  },
  "ignorePatterns": [
    "!**/*",
    "public",
    ".cache",
    "node_modules",
    "test-setup.ts",
    "metro.config.js",
    "babel.config.js"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
