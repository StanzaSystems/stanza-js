{
  "name": "next-node",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/next-node",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/next-node"
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs next-node {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/next-node/**/*.ts",
          "packages/next-node/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/next-node"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/next-node"
      }
    }
  },
  "tags": []
}
